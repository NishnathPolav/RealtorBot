# Server Configuration
PORT=5001
NODE_ENV=development

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# IBM Watsonx Discovery Configuration
WATSON_DISCOVERY_URL=https://your-watson-discovery-url.databases.appdomain.cloud
WATSON_DISCOVERY_PORT=31709
WATSON_DISCOVERY_USERNAME=your-watson-username
WATSON_DISCOVERY_PASSWORD=your-watson-password
WATSON_DISCOVERY_UI_ENDPOINT=https://your-watson-ui-endpoint.appdomain.cloud

# Google OAuth (to be configured later)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Collections
USERS_COLLECTION=users
PROPERTIES_COLLECTION=properties 