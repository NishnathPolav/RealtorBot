# Server Configuration
PORT=5001
NODE_ENV=development

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# IBM Watsonx Discovery Configuration
WATSON_DISCOVERY_URL=https://your-watson-discovery-url.databases.appdomain.cloud
WATSON_DISCOVERY_PORT=31709
WATSON_DISCOVERY_USERNAME=your-watson-username
WATSON_DISCOVERY_PASSWORD=your-watson-password
WATSON_DISCOVERY_UI_ENDPOINT=https://your-watson-ui-endpoint.appdomain.cloud

# IBM Watsonx Assistant Configuration
WATSONX_ASSISTANT_URL=https://api.us-south.assistant.watson.cloud.ibm.com/instances/cdccb4ae-8e9d-4fca-a786-2653483ac155
WATSONX_ASSISTANT_API_KEY=your-watsonx-assistant-api-key
# For new-style Watsonx Assistant, set WATSONX_ASSISTANT_ID to your environment ID (Live or Draft)
WATSONX_ASSISTANT_ID=your-watsonx-assistant-environment-id

# Google OAuth (to be configured later)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Collections
USERS_COLLECTION=users
PROPERTIES_COLLECTION=properties
TOURS_COLLECTION=tours

